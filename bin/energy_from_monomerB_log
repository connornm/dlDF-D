#!/bin/bash
# Reads the energy from a Gaussian SYSTEM.log file.
# Looks for the line with linkey variable, and reads off the number
# inbetween the startkey and endkey variables
# Usage:
#	bin/energy_from_monomerB_log SYSTEM

pathtolog="data/log/monomerB/"
system=${1}
linekey="SCF Done:"
startkey="E(RM05)="
endkey="A"

cat ${pathtolog}${system}"_monomerB.log" | grep "$linekey" | sed s/'\ '//g | sed s/'.*'"$startkey"// | sed s/"$endkey"'.*'//
